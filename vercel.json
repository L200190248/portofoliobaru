{
  "version": 2,
  "functions": {
    "api/index.php": { 
      "runtime": "vercel-php@0.7.3" 
    }
  },
  "routes": [
    { "src": "/build/(.*)", "dest": "/public/build/$1" },
    { "src": "/img/(.*)", "dest": "/public/img/$1" },
    { "src": "/videos/(.*)", "dest": "/public/videos/$1" },
    { "src": "/(.*)", "dest": "/api/index.php" }
  ],
  "buildCommand": "vite build",
  "outputDirectory": "public",
  "env": {
    "PHP_VERSION": "8.3",
    "APP_ENV": "production",
    "APP_DEBUG": "true",
    "APP_URL": "https://portofoliobaru-cnke2ccyf.vercel.app",
    "APP_KEY": "base64:TGaVaXwhWgjn9akhE4SCSIjGG/cQqvICUQ+PgLA3Bs0=",
    "COMPOSER_CACHE_DIR": "/tmp/composer_cache", 
    "APP_CONFIG_CACHE": "/tmp/config.php",
    "APP_EVENTS_CACHE": "/tmp/events.php",
    "APP_PACKAGES_CACHE": "/tmp/packages.php",
    "APP_ROUTES_CACHE": "/tmp/routes.php",
    "APP_SERVICES_CACHE": "/tmp/services.php",
    "VIEW_COMPILED_PATH": "/tmp",
    "CACHE_DRIVER": "array",
    "LOG_CHANNEL": "stderr",
    "SESSION_DRIVER": "cookie",
    "CLEAR_CACHE": "true"
  }
}
